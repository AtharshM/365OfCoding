https://practice.geeksforgeeks.org/problems/missing-number-in-array/0/?track=md-arrays&amp;batchId=144

#include<bits/stdc++.h>
using namespace std;
int main()
 {
     int t;
     cin>>t;
     while(t--)
     {
         int n;
         cin>>n;
         int sum=0,var=0;
         for(int i=0;i<n-1;i++)
         {
             cin>>var;
             sum+=var;
         }
         cout<<((n+1)*n)/2 - sum<<"\n";
     }
     
	
	return 0;
}
